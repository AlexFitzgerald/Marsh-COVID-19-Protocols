  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>MARSHSQL2MARSHFORMS</DataSourceName>
        <CommandText>SELECT COALESCE(B.employee_name, COALESCE(E.nick_name, E.first_name) + ' ' + E.last_name) AS employee_name, 
       B.dts, 
       B.guid,
       CASE
           WHEN B.guid IS NOT NULL
           THEN 'Survey'
           ELSE ''
       END AS Link, 
       E.card_number, 
       ei.cc2 + ' ' + cd2.name AS Department, 
       COALESCE(CASE
                    WHEN DATEDIFF(d, B.dts, GETDATE()) = 0
                    THEN 'Today'
                    WHEN DATEDIFF(d, B.dts, GETDATE()) = 1
                    THEN CAST(DATEDIFF(d, B.dts, GETDATE()) AS VARCHAR(10)) + ' Day ago'
                    ELSE CAST(DATEDIFF(d, B.dts, GETDATE()) AS VARCHAR(10)) + ' Days ago'
                END, 'Never been surveyed.') AS last_surveyed, 
       DATEDIFF(d, B.dts, GETDATE()) AS last_survered_days
FROM
(
    SELECT employee_badge_id, 
           MAX(dts) AS max_dts
    FROM form_temp_answers
    WHERE employee_badge_id &gt; 0
    GROUP BY employee_badge_id
) AS A
INNER JOIN form_temp_answers AS B ON A.employee_badge_id = B.employee_badge_id
                                     AND A.max_dts = B.dts
RIGHT OUTER JOIN employees AS E ON E.card_number = A.employee_badge_id
LEFT JOIN Millennium.dbo.Einfo ei ON ei.id = e.payroll_id
LEFT JOIN Millennium.dbo.CDept2 cd2 ON cd2.co = ei.co
                                       AND cd2.cc2 = ei.cc2
WHERE E.active = 1
ORDER BY B.dts</CommandText>
      </Query>
