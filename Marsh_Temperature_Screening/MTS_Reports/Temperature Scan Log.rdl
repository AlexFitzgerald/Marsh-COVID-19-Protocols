  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>MARSHSQL2_MTS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@mac">
            <Value>=Parameters!mac.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@START">
            <Value>=Parameters!START.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@END">
            <Value>=Parameters!END.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT t.badge_id, 
       t.date_time_stamp, 
       t.calculated_object_temperature_f, 
       t.fever_status, 
       d.station_name, 
       d.quadrant, 
       d.station_description, 
       e.payroll_id, 
       t.mac_address, 
       COALESCE(e.first_name, '') + ' ' + LEFT(COALESCE(e.middle_name, ''), 1) + ' ' + COALESCE(e.last_name, '') AS employee_name, 
       pe.department_number
FROM mts_temperatures AS t
     LEFT OUTER JOIN mts_devices AS d ON t.mac_address = d.mac_address
     LEFT OUTER JOIN employees AS e ON t.badge_id = e.card_number
     LEFT JOIN view_payroll_employees pe ON pe.id = e.payroll_id
WHERE t.mac_address IN(@mac)
     AND CAST(t.date_time_stamp AS DATE) BETWEEN @START AND @END</CommandText>
      </Query>
