    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>MARSHSQL2_MTS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DATE">
            <Value>=Parameters!DATE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DEPT">
            <Value>=Parameters!DEPT.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH cte
     AS (SELECT e_department, 
                s_co, 
                s_id, 
                COUNT(*) AS dep_count
         FROM MarshForms.dbo.OrgChart
         WHERE s_id IS NOT NULL
         GROUP BY e_department, 
                  s_co, 
                  s_id)
     SELECT COALESCE(e.nickname, e.firstName) + ' ' + e.lastName AS supervisor_name, 
            e.emailAddress, 
            a.e_department, 
            COALESCE(p.nickname, p.firstName) + ' ' + p.lastName AS employee_name, 
            p.id, 
            p.co, 
            emp.card_number, 
            clock.first_clock_time,
			temp.calculated_object_temperature_f,
			temp.date_time_stamp
     FROM cte
          INNER JOIN
     (
         SELECT e_department, 
                MAX(dep_count) AS dep_count
         FROM cte
         GROUP BY e_department
     ) a ON cte.e_department = a.e_department
            AND cte.dep_count = a.dep_count
          INNER JOIN view_payroll_employees e ON e.co = cte.s_co
                                                 AND e.id = cte.s_id
          INNER JOIN view_payroll_employees p ON p.department_number = a.e_department
          LEFT JOIN employees emp ON emp.payroll_id = p.id
          LEFT JOIN
     (
         SELECT MIN(CASE
                        WHEN COALESCE(OutUnrounded, '2999-01-01') &lt; COALESCE(InUnrounded, '2999-01-01')
                        THEN OutUnrounded
                        ELSE InUnrounded
                    END) AS first_clock_time, 
                PunchDate, 
                EmployeeCode
         FROM MarshCredits.FP.Punches
         WHERE PunchDate = @DATE
               AND Category = 'Regular'
               AND Type = 'Times'
         GROUP BY PunchDate, 
                  EmployeeCode
     ) AS clock ON clock.EmployeeCode = p.id

	 left join (
	 Select b.* from (
SELECT badge_id, 
       MIN(date_time_stamp) AS min_date_time_stamp
FROM MarshForms.dbo.mts_temperatures t
WHERE CAST(t.date_time_stamp AS DATE) = @DATE
										 group by badge_id
										 ) as a inner join mts_temperatures b on a.badge_id = b.badge_id and a.min_date_time_stamp = b.date_time_stamp
	 
	 ) as temp on temp.badge_id = emp.card_number

	 where p.department_number in (@DEPT)</CommandText>
      </Query>
